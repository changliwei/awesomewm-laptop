! Xft settings ---------------------------------------------------------------

Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight

!-------------------------------------------------------------------------------
!
!	U R X V T - Settings
!
URxvt*saveLines: 3100
URxvt*geometry:					80x25
URxvt*borderLess:				false
URxvt*depth:					32
URxvt*scrollBar_right:				false
URxvt*scrollBar:		      false
URxvt*cursorBlink:				true
URxvt*cursorUnderline:				true
URxvt*scrollColor:				#111111
URxvt*internalborder: 6

! rxvt-unicode
URxvt*urgentOnBell:  true

! Black
*color0:   #111111
*color8:   #666666

! Red
*color1:   rgb:d3/62/65
*color9:   rgb:ef/81/71

! Green
*color2:   #aece91
*color10:  #cfefb3

! Yellow
*color3:   rgb:e7/e1/8c
*color11:  rgb:ff/f7/96

! Blue
*color4:   #5297cf
*color12:  #74b8ef

! Magenta
*color5:   #963c59
*color13:  #b85e7b

! Cyan
*color6:   #5E7175
*color14:  #A3BABF

! White
*color7:   #bebebe
*color15:  #ffffff

! enable clickable links
URxvt*urlLauncher: firefox
URxvt*matcher.button: 1
URxvt*matcher.pattern.1: \\b(mailto|http|https|ftp|file):[/]*[\\w-]\\.[\\w./?&@#-]*[\\w/-]
URxvt*matcher.pattern.2: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]

URxvt*tabbed.tabbar-fg: 11
URxvt*tabbed.tabbar-bg: 0
URxvt*tabbed.tab-fg:    14
URxvt*tabbed.tab-bg:    0

! include font and bg/fg colors
# include ".config/awesome/.urxvt_font"